    <TextView
        app:layout_constraintTop_toBottomOf="@+id/zongjie"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/text_zhanbu"
        android:layout_marginLeft="9dp"
        android:layout_marginTop="12dp"
        android:textColor="#B89EEC"
        android:text="占卜"
        android:textSize="18sp"
        android:layout_width="36dp"
        android:layout_height="25dp"/>
    <ListView
        android:id="@+id/listview_zhanbu"
        app:layout_constraintTop_toBottomOf="@+id/text_zhanbu"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_width="match_parent"
        android:dividerHeight="15dp"
        android:layout_height="wrap_content"/>
    <GridView
        android:id="@+id/grid_zhanbu"
        app:layout_constraintTop_toBottomOf="@+id/listview_zhanbu"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_width="match_parent"
        android:numColumns="2"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:horizontalSpacing="11dp"
        android:layout_marginTop="15dp"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/more_test"
        app:layout_constraintTop_toBottomOf="@id/grid_zhanbu"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="9dp"
        android:layout_marginLeft="18dp"
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#B89EEC"
        android:text="更多测试"/>
    <GridView
        android:id="@+id/grid_more_test"
        app:layout_constraintTop_toBottomOf="@+id/more_test"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:horizontalSpacing="11dp"
        android:numColumns="2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
